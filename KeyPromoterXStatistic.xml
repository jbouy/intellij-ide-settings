<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Debug">
        <Item>
          <IdeaActionID>ActivateDebugToolWindow</IdeaActionID>
          <ShortCut>'Alt+5'</ShortCut>
          <Description>Debug</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'TurnerTablet.Api'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'Alt+F5'</ShortCut>
          <Description>Debug 'TurnerTablet.Api'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Errors In Solution">
        <Item>
          <IdeaActionID>ActivateErrorsInSolutionToolWindow</IdeaActionID>
          <ShortCut>'Ctrl+Alt+2'</ShortCut>
          <Description>Errors In Solution</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'TurnerTablet.Api'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Shift+F5'</ShortCut>
          <Description>Stop 'TurnerTablet.Api'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="TODO">
        <Item>
          <IdeaActionID>ActivateTODOToolWindow</IdeaActionID>
          <ShortCut>'Alt+6'</ShortCut>
          <Description>TODO</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Unit Tests">
        <Item>
          <IdeaActionID>ActivateUnitTestsToolWindow</IdeaActionID>
          <ShortCut>'Alt+8' or 'Alt+Shift+8'</ShortCut>
          <Description>Unit Tests</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>